# Discord Bot Token (get this from https://discord.com/developers/applications)
DISCORD_BOT_TOKEN=your_bot_token_here

# Auto Role ID (the role ID that will be assigned to new members)
# To get the role ID: Enable Developer Mode in Discord, right-click the role, and click "Copy ID"
AUTO_ROLE_ID=your_role_id_here

# Optional: Welcome Channel ID (uncomment if you want welcome messages)
# WELCOME_CHANNEL_ID=your_channel_id_here

